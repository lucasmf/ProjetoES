<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meusmedicos"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/meus_medicos"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".views.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.consulta.AddConsultasActivity"
           		  android:label="@string/activity_consultas_message" 
           		  android:parentActivityName=".views.consulta.ShowConsultas" >
        </activity>
        <activity android:name=".views.consulta.ShowConsultas"
           		  android:label="@string/activity_consultas_message" 
           		  android:parentActivityName=".views.MainActivity" >
        </activity>
        <activity android:name=".views.consulta.AddAnnotationOnConsulta"
                  android:label="@string/activity_anotacoes_message"
                  android:parentActivityName=".views.consulta.ShowConsultaDetalhada">
        </activity>
        <activity android:name=".views.consulta.ShowConsultaDetalhada"
                  android:label="@string/activity_consultas_message"
                  android:parentActivityName=".views.consulta.ShowConsultas">
        </activity>
        <activity android:name=".views.sintoma.ShowSintomas"
                  android:label="@string/activity_sintomas_message"
                  android:parentActivityName=".views.MainActivity"/>
        <activity
            android:name=".views.sintoma.AddSintomasActivity"
            android:label="Sintoma" />
    </application>

</manifest>
